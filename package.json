    "local": "WEBPACK_ENV=local webpack-dashboard --  webpack-dev-server --progress",
    "start": "WEBPACK_ENV=dev webpack-dev-server --progress",
    "www": "rm -rf www/* && WEBPACK_ENV=prod webpack --progress && serve -p 9001 www/",
    "deploy-local": "rm -rf www && WEBPACK_ENV=prod webpack --progress && ./config/deploy/local.sh",
    "deploy-dev": "rm -rf www && WEBPACK_ENV=prod webpack --progress && ./config/deploy/dev.sh",
    "deploy-test": "rm -rf www && WEBPACK_ENV=test webpack --progress && ./config/deploy/test.sh && ./config/cdn/test.sh",
